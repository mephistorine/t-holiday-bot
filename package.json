{
  "name": "t-holydays-bot",
  "version": "1.0.0",
  "scripts": {
    "build": "rimraf dist && esbuild --outdir=dist --sourcemap --bundle --minify --platform=node --target=node20.10.0 ./src/index.ts",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "dev": "nodemon --watch src --watch test --ext ts,json --exec 'pnpm bundle && pnpm start'",
    "debug": "pnpm build && node --enable-source-maps --inspect-brk ./dist/index.js",
    "debug:test": "node --inspect-brk ./node_modules/.bin/jest --runInBand .",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "start": "node --enable-source-maps ./dist/index.js",
    "test": "jest --runInBand ."
  },
  "main": "dist/index.js",
  "author": "Sam Bulatov <mephistorine@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@babel/preset-typescript": "7.24.7",
    "@jest/globals": "29.7.0",
    "@tsconfig/node20": "20.1.4",
    "@types/jest": "29.5.12",
    "@types/node": "20.16.1",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "esbuild": "0.21.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "nodemon": "3.1.4",
    "prettier": "3.3.3",
    "rimraf": "5.0.10",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@fastify/env": "4.4.0",
    "@fastify/formbody": "7.4.0",
    "@fastify/redis": "6.2.0",
    "@fastify/sensible": "5.6.0",
    "@sweet-monads/either": "3.3.1",
    "cheerio": "1.0.0",
    "fastify": "4.28.1",
    "redis": "4.7.0"
  }
}
